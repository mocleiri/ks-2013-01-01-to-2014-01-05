<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="KS-CourseView-ReviewProposalPage" parent="Uif-MaintenancePage"
		p:headerText="Review Proposal" p:disclosure.render="false">
        <property name="header" ref="KS-CourseView-Header" />

        <property name="items">
            <list>

                <bean parent="Uif-Disclosure-VerticalBoxSection">
                    <property name="items">
                        <list>
                            <bean parent="KS-CourseView-SectionHeader">
                                <property name="headerText" value="Course Information" />
                            </bean>
                            <ref bean="KS-CourseView-CourseInfoPage-Widgets" />
                        </list>                        
                    </property>
                </bean>

                <bean parent="Uif-Disclosure-VerticalBoxSection">
                    <property name="items">
                        <list>
                            <bean parent="KS-CourseView-SectionHeader">
                                <property name="headerText" value="Governance" />
                            </bean>
                            <ref bean="KS-CourseView-GovernancePage-Widgets" />
                        </list>
                    </property>
                </bean>

                <bean parent="Uif-Disclosure-VerticalBoxSection">
                    <property name="items">
                        <list>
                            <bean parent="KS-CourseView-SectionHeader">
                                <property name="headerText" value="Course Logistics" />
                            </bean>
                            <ref bean="KS-CourseView-CourseLogisticsPage-Scheduling-Widgets" />
                            <ref bean="KS-CourseView-CourseLogisticsPage-DurationCount-Widgets" />
                            <ref bean="KS-CourseView-CourseLogisticsPage-LearningResults-Widgets" />
                            <ref bean="KS-CourseView-CourseLogisticsPage-GradesAssessments-Widgets" />
                            <ref bean="KS-CourseView-CourseLogisticsPage-StudentRegistrationOptions-Widgets" />
                            <ref bean="KS-CourseView-CourseLogisticsPage-FinalExam-Widgets" />
                            <ref bean="KS-CourseView-CourseLogisticsPage-Outcome-Widgets" />
                            <ref bean="KS-CourseView-CourseLogisticsPage-CourseFormats-Widgets" />
                        </list>
                    </property>
                </bean>

                <bean parent="Uif-Disclosure-VerticalBoxSection">
                    <property name="items">
                        <list>
                            <bean parent="KS-CourseView-SectionHeader">
                                <property name="headerText" value="Learning Objectives" />
                            </bean>
                            <ref bean="LearningObjective-CollectionSection" />
                        </list>
                    </property>
                </bean>

                <bean parent="Uif-Disclosure-VerticalBoxSection">
                    <property name="items">
                        <list>
                            <bean parent="KS-CourseView-SectionHeader">
                                <property name="headerText" value="Active Dates" />
                            </bean>
                            <ref bean="KS-CourseView-ActiveDatesPage-Widgets" />
                        </list>
                    </property>
                </bean>

                <bean parent="Uif-Disclosure-VerticalBoxSection">
                    <property name="items">
                        <list>
                            <bean parent="KS-CourseView-SectionHeader">
                                <property name="headerText" value="Authors and Collaborators"/>
                            </bean>

                            <ref bean="KS-CourseView-AuthorsAndCollaboratorsPage-Widgets" />
                        </list>
                    </property>
                </bean>

                <bean parent="Uif-Disclosure-VerticalBoxSection">
                    <property name="items">
                        <list>
                            <bean parent="KS-CourseView-SectionHeader">
                                <property name="headerText" value="Supporting Documents" />
                            </bean>

                            <ref bean="KS-CourseView-SupportingDocumentsPage-Widgets" />
                        </list>
                    </property>
                </bean>

            </list>
        </property>

        <property name="footer">
            <bean parent="KS-CourseView-Footer"/>
        </property>	
    </bean>
</beans>
