<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <import resource="classpath:ks-CourseInfo-dictionary.xml" />
    <import resource="classpath:ks-ProposalInfo-dictionary.xml" />

    <!-- CourseInfo -->
    <bean id="CourseProposalInfo" parent="DataObjectEntry">
        <property name="name" value="courseProposalInfo" />
        <property name="dataObjectClass" value="org.kuali.student.cm.course.dto.CourseProposalInfo" />
        <property name="objectLabel" value="Course and Proposal Wrapper" />
        <property name="objectDescription">
            <value>
                This class wraps both the CourseInfo and ProposalInfo objects in order to maintain it using CM
                services.
            </value>
        </property>
        <!--
        <property name="titleAttribute" value="course.courseTitle" />
        -->
        <property name="primaryKeys">
            <list>
                <value>id</value>
            </list>
        </property>

        <property name="attributes">
            <list>
                <ref bean="CourseProposalInfo.audit" />
                <ref bean="CourseProposalInfo.passFail" />
                <ref bean="CourseProposalInfo.finalExamStatus" />
                <ref bean="CourseProposalInfo.finalExamRationale" />
                <ref bean="CourseProposalInfo.showAll" />
                <ref bean="CourseProposalInfo.userId" />
                <ref bean="CourseProposalInfo.lastUpdated" />
            </list>
        </property>

        <!--
        <property name="complexAttributes">
            <list>
                <ref bean="CourseProposalInfo.course" />
                <ref bean="CourseProposalInfo.proposal" />
                <ref bean="CourseProposalInfo.loDialogWrapper" />
            </list>
        </property>

        <property name="collections">
            <list>
                <ref bean="CourseProposalInfo.instructorWrappers" />
                <ref bean="CourseProposalInfo.courseJointWrappers" />
                <ref bean="CourseProposalInfo.creditOptionWrappers" />
                <ref bean="CourseProposalInfo.commentInfos" />
                <ref bean="CourseProposalInfo.decisions" />
                <ref bean="CourseProposalInfo.administeringOrganizations" />
            </list>
        </property>
        -->
    </bean>

    <!-- Attributes -->

    <bean id="CourseProposalInfo.audit" parent="BaseKuali.boolean">
        <property name="name" value="audit" />
        <property name="shortLabel" value="Audit" />
        <property name="summary">
            <value>
                Flag to indicate if this course can be audited.
            </value>
        </property>
        <property name="label" value="Audit" />
        <property name="description">
            <value>
                Flag to indicate if this course can be audited.
            </value>
        </property>
        <property name="required" value="false" />
    </bean>

    <bean id="CourseProposalInfo.passFail" parent="BaseKuali.boolean">
