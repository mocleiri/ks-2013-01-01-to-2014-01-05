        </property>
        <property name="required" value="false" />
    </bean>

    <bean id="CourseProposalInfo.showAll" parent="BaseKuali.boolean">
        <property name="name" value="showAll" />
        <property name="shortLabel" value="Show All" />
        <property name="summary">
            <value>
                Show All Required fields.
            </value>
        </property>
        <property name="label" value="Show All" />
        <property name="description">
            <value>
                Show All Required fields.
            </value>
        </property>
        <property name="required" value="false" />
    </bean>

    <bean id="CourseProposalInfo.userId" parent="BaseKuali.string">
        <property name="name" value="userId" />
        <property name="shortLabel" value="User Id" />
        <property name="summary">
            <value>
                User id of the currently logged in user.
            </value>
        </property>
        <property name="label" value="User Id" />
        <property name="description">
            <value>
                User id of the currently logged in user.
            </value>
        </property>
        <property name="required" value="false" />
    </bean>

    <bean id="CourseProposalInfo.lastUpdated" parent="BaseKuali.string">
        <property name="name" value="lastUpdated" />
        <property name="shortLabel" value="Last Updated" />
        <property name="summary">
            <value>
                Date and Time of last updated change to the course proposal.
            </value>
        </property>
        <property name="label" value="Last Updated" />
        <property name="description">
            <value>
                Date and Time of last updated change to the course proposal.
            </value>
        </property>
        <property name="required" value="false" />
    </bean>

