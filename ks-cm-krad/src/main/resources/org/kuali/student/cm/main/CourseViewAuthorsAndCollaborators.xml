<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="KS-CourseView-AuthorsAndCollaboratorsPage" parent="Uif-Page"
		p:headerText="Authors And Collaborators" p:disclosure.render="false">
		<property name="header">
			<bean parent="Uif-HeaderTwo">
				<property name="cssClasses">
					<list merge="true">
						<value>ks-header</value>
					</list>
				</property>
			</bean>
		</property>
<!-- 		<property name="items"> -->
<!-- 			<list> -->
<!-- 				<ref bean="KS-CourseView-AuthorsAndCollaboratorsPage-Widgets" /> -->
<!-- 			</list> -->
<!-- 		</property> -->
		<property name="footer">
			<bean parent="Uif-FooterBase">
				<property name="items">
					<list>
						<bean parent="Uif-ActionLink" p:actionLabel="Cancel"
							p:actionScript="stepBrowserBack()" />
					</list>
				</property>
			</bean>
		</property>
	</bean>
</beans>