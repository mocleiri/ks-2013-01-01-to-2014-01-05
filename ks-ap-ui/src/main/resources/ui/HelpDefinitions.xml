    <bean id="quarter_facet_help_icon" parent="MyPlanHelpImage">
        <property name="toolTip">
          <bean parent="MyPlan-Tooltip-Left" p:tooltipContent="#{T(org.kuali.student.ap.framework.config.KsapFrameworkServiceLocator).getTextHelper().getText('DETAILS_TERMS_TOOLTIP_TEXT')}"/>
        </property>
    </bean>


    <bean id="bookmarks_help_icon" parent="MyPlanHelpImage">
        <property name="toolTip">
          <bean parent="MyPlan-Tooltip-Right" p:tooltipContent="Bookmark a course to save it for future planning. The course remains in your bookmarks until you've added it to your Plan or deleted it."/>
        </property>
    </bean>


    <bean id="recent_audits_help_icon" parent="MyPlanHelpImage">
         <property name="toolTip">
          <bean parent="MyPlan-Tooltip-Right" p:tooltipContent="Displays the last seven audits run through MyPlan. Audits are cleared at the end of each quarter."/>
        </property>
    </bean>


    <bean id="completed_help_icon" parent="MyPlanHelpImage"
          p:render="@{#parentLine.completedTerm and #parentLine.displayCompletedHelp ? true : false}">
        <property name="toolTip">
          <bean parent="MyPlan-Tooltip" p:tooltipContent="Courses that you registered for in a quarter that is now complete."/>
        </property>
    </bean>

    <bean id="registered_help_icon" parent="MyPlanHelpImage"
          p:render="@{!#parentLine.completedTerm and (#parentLine.academicRecord.size() gt 0 or !#parentLine.openForPlanning) and #parentLine.displayRegisteredHelp ? true : false}">
        <property name="toolTip">
            <bean parent="MyPlan-Tooltip-Left"
                  p:tooltipContent="Courses you are enrolled in for the current or a future academic quarter. To update this information, go to the &lt;a target=_blank href=&quot;https://sdb.admin.washington.edu/students/uwnetid/register.asp&quot;&gt;Registration&lt;/a&gt; page."/>
        </property>
    </bean>

    <bean id="cart_help_icon" parent="MyPlanHelpImage" p:render="@{#line.displayCartHelp ? true : false}">
        <property name="toolTip">
            <bean parent="MyPlan-Tooltip" p:tooltipContent="Items in the cart are those you have selected to register for but have not been submitted for registration yet."/>
        </property>
    </bean>

    <bean id="planned_help_icon" parent="MyPlanHelpImage" p:render="@{#line.displayPlannedHelp}">
        <property name="toolTip">
            <bean parent="MyPlan-Tooltip-Left"
                  p:tooltipContent="MyPlan allows you to add courses to your plan, but does not register you. Check the Time Schedule each quarter to verify whether your planned courses will be offered. &lt;a href=javascript:openHelpWindow(&quot;http://depts.washington.edu/myplan/help-site/#825&quot;);&gt;Learn more&lt;/a&gt;"/>
        </property>
    </bean>

    <bean id="backup_help_icon" parent="MyPlanHelpImage" p:render="@{#line.displayBackupHelp}">
        <property name="toolTip">
            <bean parent="MyPlan-Tooltip-Left"
                  p:tooltipContent="Courses you are considering in case a planned course is full or unavailable."/>
        </property>
    </bean>

    <bean id="total_credits_help_icon" parent="MyPlanHelpImage" p:render="@{#line.displayCreditsHelp}">
        <property name="toolTip">
            <bean parent="MyPlan-Tooltip-Left"
                  p:tooltipContent="@{!#line.openForPlanning ? 'The number of credits registered for a completed quarter. Check your transcript to verify completed credits.' : (#line.academicRecord.size() gt 0 ? ' The total number of credits from your planned and registered courses for the current quarter.' : 'The number of credits you plan to enroll in for future quarter.')}"/>
        </property>
    </bean>

    <bean id="cart_credits_help_icon" parent="MyPlanHelpImage" p:render="@{#line.displayCreditsHelp ? true : false}">
        <property name="toolTip">
            <bean parent="MyPlan-Tooltip" p:tooltipContent="The total number of credits for all course currently in the shopping cart.">
            </bean>
        </property>
    </bean>

    <bean id="course_requisite_help_icon" parent="MyPlanHelpImage">
        <property name="toolTip">
            <bean parent="MyPlan-Tooltip-Left"
                  p:tooltipContent="#{T(org.kuali.student.ap.framework.config.KsapFrameworkServiceLocator).getTextHelper().getText('DETAILS_REQUISITE_TOOLTOP_COREQ_TEXT')}
                                    #{T(org.kuali.student.ap.framework.config.KsapFrameworkServiceLocator).getTextHelper().getText('DETAILS_REQUISITE_TOOLTOP_PREREQ_TEXT')}"/>
        </property>
    </bean>

    <bean id="course_quarter_help_icon" parent="MyPlanHelpImage">
        <property name="toolTip">
            <bean parent="MyPlan-Tooltip-Left"
                  p:tooltipContent="#{T(org.kuali.student.ap.framework.config.KsapFrameworkServiceLocator).getTextHelper().getText('DETAILS_TERMS_TOOLTIP_TEXT')}"/>
        </property>
    </bean>

    <bean id="course_credits_help_icon" parent="MyPlanHelpImage">
        <property name="toolTip">
            <bean parent="MyPlan-Tooltip-Left"
                  p:tooltipContent="&lt;strong&gt;Variable Credits&lt;/strong&gt; &lt;/br&gt;
                                                                 &lt;strong&gt;(1-5)-&lt;/strong&gt; You can enroll for any number of credits in the range that's given. These are typically independent study courses that you and the professor decide on the number of credits.&lt;/br&gt;&lt;/br&gt;
                                                                 &lt;strong&gt;(3/5) -&lt;/strong&gt; A course you can enroll in for either 3 or 5 credits."/>
        </property>
    </bean>

    <bean id="course_gened_help_icon" parent="MyPlanHelpImage">
        <property name="toolTip">
            <bean parent="MyPlan-Tooltip-Left"
                  p:tooltipContent="#{T(org.kuali.student.ap.framework.config.KsapFrameworkServiceLocator).getTextHelper().getText('DETAILS_GENED_TOOLTIP_TEXT')}"/>
        </property>
    </bean>

    <bean id="course_sections_help_icon" parent="MyPlanHelpImage">
        <property name="toolTip">
            <bean parent="MyPlan-Tooltip-Right"
                  p:tooltipContent="&lt;strong&gt;ENRL/LIMIT&lt;/strong&gt; = section enrollment and overall enrollment limit&lt;/br&gt;
                  &lt;strong&gt;ENRL. RESTR.&lt;/strong&gt; = applicable enrollment restrictions for each section&lt;/br&gt;
                  &lt;strong&gt;OTHER&lt;/strong&gt; = additional information for each section&lt;/br&gt;
                  &lt;strong&gt;PCE&lt;/strong&gt; = Professional &amp; Continuing Education&lt;/br&gt;
                  "/>
        </property>
    </bean>

</beans>
