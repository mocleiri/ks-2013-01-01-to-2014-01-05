    <!-- #################################### Planned Courses Term View ########################### -->

    <!--Mock Plan View-->
    <bean id="SnapshotCourses-FormView" parent="SnapshotCourses-FormView-parent" />
    <bean abstract="true" id="SnapshotCourses-FormView-parent" parent="MyPlanFormView">
        <property name="id" value="SnapshotCourses-FormView"/>
        <property name="cssClasses">
            <list merge="true">
                <value>myplan-view</value>
                <value>ks-plan-View</value>
            </list>
        </property>
        <property name="additionalScriptFiles">
            <list>
                <value>ks-myplan/plugins/jcarousellite.js</value>
                <value>arm-ks/scripts/arm-review-ks.js</value>
                <value>arm-ks/scripts/review/dotdotdot/jquery.dotdotdot.min.js</value>
            </list>
        </property>
        <property name="additionalCssFiles">
            <list>
                <value>arm-ks/css/arm-review-ks.css</value>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-Page" p:id="plan-page">
                    <property name="cssClasses">
                        <list merge="true">
                            <value>ks-plan-View-page</value>
                        </list>
                    </property>
                    <property name="layoutManager">
                        <bean parent="Uif-HorizontalBoxLayout"/>
                    </property>
                    <property name="items">
                        <list>
                            <ref bean="snapshot_mock_detail"/>
                            <ref bean="SnapshotCourses_myplan_section"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
        <property name="footer.render" value="false"/>
        <property name="formClass" value="org.kuali.student.ap.plan.review.form.ConversationListForm"/>

    </bean>

    <bean id="SnapshotCourses_myplan_section" parent="SnapshotCourses_myplan_section-parent" />
    <bean id="SnapshotCourses_myplan_section-parent" abstract="true" parent="myplan_section">
        <property name="cssClasses">
            <list merge="true">
                <value>ks-plan-Widgets</value>
            </list>
        </property>
        <property name="items">
            <list>
                <bean p:id="bookmark_list" parent="Uif-VerticalBoxGroup" p:headerText="Bookmarks">
                    <property name="header">
                        <bean parent="Uif-HeaderSix"/>
                    </property>
                    <property name="onDocumentReadyScript" value="
                        myplanRetrieveComponent('bookmark_list','snapshot_bookmarks_summary','search','lookup',{viewId:'SnapshotBookmarkSummary-LookupView'});"/>
                </bean>
                <bean p:id="conversation_list" parent="Uif-VerticalBoxGroup" p:headerText="Conversations">
                    <property name="header">
                        <bean parent="Uif-HeaderSix"/>
                    </property>
                    <property name="onDocumentReadyScript" value="
                        myplanRetrieveComponent('conversation_list','conversation_view','load','review/view',{viewId:'Conversation-FormView',learningPlanId:'@{initialRequestParameters['learningPlanId']}'});"/>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="snapshot_mock_detail" parent="Uif-VerticalBoxSubSection">
        <property name="cssClasses">
            <list merge="true">
                <value>myplan-plan-loader</value>
                <value>ks-plan-Loader</value>
            </list>
        </property>
        <property name="validationMessages.displayMessages" value="false"/>
        <property name="onDocumentReadyScript" value="
            var retrieveOptions = {};
            var sFocusAtpId = '';
            var aParams = window.location.search.replace('?','').split('&amp;');
            jQuery.each(aParams, function(index, value) {
                if (value.split('=')[0].indexOf('focusAtpId') >= 0) {
                    sFocusAtpId = decodeURIComponent(value.split('=')[1]);
                }
            });
            if(sFocusAtpId != ''){
                retrieveOptions = {viewId:'SnapshotCourses-LookupView', focusAtpId:sFocusAtpId};
            } else {
                retrieveOptions = {viewId:'SnapshotCourses-LookupView'};
            }
            myplanRetrieveComponent('snapshot_mock_detail','snapshot_courses_detail','search','lookup', retrieveOptions, null, {message:'&lt;p&gt;&lt;img src=&quot;@{#ConfigProperties['ks.myplan.externalizable.images.url']}ajaxAuditRunning32.gif&quot; alt=&quot;loading...&quot; /&gt;&lt;/p&gt;&lt;p&gt;Please wait while we are fetching your plan...&lt;/p&gt;', fadeIn:0, fadeOut:0});
        "/>
    </bean>

    <!--
      This lookup provides the term by term view of Planned Courses.
    -->
    <bean id="SnapshotCourses-LookupView" parent="MyPlanBaseLookupView">
        <property name="headerText" value="Plan"/>
        <property name="header.render" value="false"/>
        <property name="viewName" value="SnapshotCourses-LookupView"/>
        <property name="dataObjectClassName" value="org.kuali.student.myplan.plan.dataobject.PlannedTerm"/>
        <property name="viewHelperServiceClass"
                  value="org.kuali.student.myplan.plan.service.PlannedCoursesLookupableHelperImpl"/>
        <property name="singlePageView" value="true"/>
        <property name="page.header.render" value="false"/>
        <property name="cssClasses">
            <list merge="true">
                <value>myplan-view</value>
            </list>
        </property>
        <property name="additionalScriptFiles">
            <list>
                <value>ks-myplan/plugins/jcarousellite.js</value>
            </list>
        </property>
        <property name="page">
            <bean parent="Uif-LookupPage" p:id="snapshot_courses_detail_page">
                <property name="layoutManager">
                    <bean parent="Uif-HorizontalBoxLayout"/>
                </property>
                <property name="items">
                    <list>
                        <ref bean="snapshot_courses_detail"/>
                    </list>
                </property>
            </bean>
        </property>

        <property name="criteriaFields">
            <list/>
        </property>
        <property name="resultFields">
            <list/>
        </property>
        <property name="footer.render" value="false"/>
    </bean>

    <!-- Single term view that lists planned course, total credits and backup courses -->
    <bean id="snapshot_courses_detail" parent="snapshot_courses_detail_parent" />
    <bean id="snapshot_courses_detail_parent" parent="Uif-VerticalBoxSubSection" abstract="true" p:cssClasses="ks-plan-View-pageDetails">
        <property name="onDocumentReadyScript" value="
            if ( jQuery('#snapshot_courses_detail_list ul:not(.errorLines) li').length > 0 ) {
                var iMaxHeight = Math.max.apply(null, jQuery('#snapshot_courses_detail_list ul:not(.errorLines) li').map(function() {
                    return jQuery(this).height();
                }).get());
                jQuery('#snapshot_courses_detail_list ul:not(.errorLines) li').height(iMaxHeight);
                var iStart = 0;
                if (readUrlHash('planView')) {
                    iStart = parseFloat(readUrlHash('planView'));
                } else {
                    if ( jQuery(&quot;div:hidden[id^='atp_start_index']&quot;).length > 0 ) {
                        iStart = parseFloat(jQuery(&quot;div:hidden[id^='atp_start_index']&quot;).text());
                    }

                }

                jQuery('#snapshot_courses_detail_list').jCarouselLite({
                    btnNext: '.myplan-carousel-next',
                    btnPrev: '.myplan-carousel-prev',
                    scroll: @{T(org.kuali.student.ap.framework.config.KsapFrameworkServiceLocator).getTermHelper().getNumberOfTermsInAcademicYear()},
                    visible: @{T(org.kuali.student.ap.framework.config.KsapFrameworkServiceLocator).getTermHelper().getNumberOfTermsInAcademicYear()},
                    start: iStart,
                    afterEnd: function(a) {
                        var aFirst = jQuery.trim(jQuery(a[0]).find(&quot;div:hidden[id^='plan_base_atpId']&quot;).text());
                        var aLast = jQuery.trim(jQuery(a[a.length - 1]).find(&quot;div:hidden[id^='plan_base_atpId']&quot;).text());
                        jQuery('#snapshot_courses_detail .myplan-plan-header').html(aFirst+ ' - ' + aLast);
                        setUrlHash('planView', jQuery(a[0]).index());
                    },
                    initCallback: function(a) {
                      var aFirst = jQuery.trim(jQuery(a[0]).find(&quot;div:hidden[id^='plan_base_atpId']&quot;).text());
                      var aLast = jQuery.trim(jQuery(a[a.length - 1]).find(&quot;div:hidden[id^='plan_base_atpId']&quot;).text());
                      jQuery('#snapshot_courses_detail .myplan-plan-header').html(aFirst+ ' - ' + aLast);
                      jQuery.unblockUI();
                    }
                });
            }
        "/>
        <property name="validationMessages.displayMessages" value="false"/>
        <property name="items">
            <list>
                <bean parent="Uif-HorizontalBoxGroup">
                    <property name="validationMessages.render" value="false"/>
                    <property name="cssClasses">
                        <list merge="true">
                            <value>myplan-plan-heading</value>
                            <value>ks-plan-Header</value>
                        </list>
                    </property>
                    <property name="items">
                        <list>
                            <bean parent="Uif-VerticalBoxGroup">
                                <property name="cssClasses">
                                    <list merge="true">
                                        <value>ks-plan-Header-buttonWrapper</value>
                                        <value>ks-plan-Header-buttonWrapper--previous</value>
                                    </list>
                                </property>
                                <property name="validationMessages.render" value="false"/>
                                <property name="items">
                                    <list>
                                        <bean p:id="previous_year_button" parent="Uif-PrimaryActionButton"
                                              p:actionScript="" p:title="Previous Academic Year">
                                            <property name="actionImage">
                                                <bean parent="Uif-Image"
                                                      p:source="@{#ConfigProperties['ks.myplan.externalizable.images.url']}pixel.gif">
                                                    <property name="cssClasses">
                                                        <list merge="true">
                                                            <value>ks-plan-Header-buttonImage</value>
                                                        </list>
                                                    </property>
                                                </bean>
                                            </property>
                                            <property name="cssClasses">
                                                <list merge="true">
                                                    <value>myplan-carousel-prev</value>
                                                    <value>ks-plan-Header-button</value>
                                                    <value>ks-plan-Header-button--previous</value>
                                                </list>
                                            </property>
                                        </bean>
                                    </list>
                                </property>
                            </bean>

                            <bean parent="Uif-MessageField">
                                <property name="cssClasses">
                                    <list merge="true">
                                        <value>myplan-plan-header</value>
                                        <value>ks-plan-Header-headline</value>
                                    </list>
                                </property>
                                <property name="message">
                                    <bean parent="Uif-Message" p:messageText="Academic Year"/>
                                </property>
                            </bean>

                            <bean parent="Uif-VerticalBoxGroup">
                                <property name="cssClasses">
                                    <list merge="true">
                                        <value>ks-plan-Header-buttonWrapper</value>
                                        <value>ks-plan-Header-buttonWrapper--next</value>
                                    </list>
                                </property>
                                <property name="validationMessages.render" value="false"/>
                                <property name="items">
                                    <list>
                                        <bean p:id="next_year_button" parent="Uif-PrimaryActionButton" p:actionScript=""
                                              p:title="Next Academic Year">
                                            <property name="actionImage">
                                                <bean parent="Uif-Image"
                                                      p:source="@{#ConfigProperties['ks.myplan.externalizable.images.url']}pixel.gif">
                                                    <property name="cssClasses">
                                                        <list merge="true">
                                                            <value>ks-plan-Header-buttonImage</value>
                                                        </list>
                                                    </property>
                                                </bean>
                                            </property>
                                            <property name="cssClasses">
                                                <list merge="true">
                                                    <value>myplan-carousel-next</value>
                                                    <value>ks-plan-Header-button</value>
                                                    <value>ks-plan-Header-button--next</value>
                                                </list>
                                            </property>
                                        </bean>
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
                <bean parent="Uif-HorizontalBoxGroup">
                    <property name="validationMessages.render" value="false"/>
                    <property name="items">
                        <list>
                            <bean parent="Uif-MessageField"
                                  p:messageText="View Courses in the planner snapshot."/>
                        </list>
                    </property>
                    <property name="cssClasses">
                        <list merge="true">
                            <value>ks-plan-Instructions</value>
                        </list>
                    </property>
                </bean>
                <ref bean="snapshot_courses_detail_list"/>
            </list>
        </property>
    </bean>

    <bean id="snapshot_courses_detail_list" parent="snapshot_courses_detail_list-parent" />
    <bean id="snapshot_courses_detail_list-parent" parent="Uif-ListCollectionSection">
        <property name="validationMessages.render" value="false"/>
        <property name="collectionObjectClass" value="org.kuali.student.myplan.plan.dataobject.PlannedTerm"/>
        <property name="layoutManager.lineGroupPrototype.header.render" value="true"/>
        <property name="layoutManager.lineGroupPrototype.headerText" value="@{#line.qtrYear}"/>
        <property name="layoutManager.lineGroupPrototype.validationMessages.render" value="false"/>
        <property name="layoutManager.lineGroupPrototype.cssClasses">
            <list merge="true">
                <value>uif-group</value>
                <value>uif-boxGroup</value>
                <value>uif-verticalBoxGroup</value>
                <value>uif-boxSection</value>
                <value>@{#line.currentTermForView ? 'myplan-term-current' : (!#line.currentTermForView and
                    #line.openForPlanning ? 'myplan-term-future' : 'myplan-term-past')}
                </value>
                <value>@{#line.openForPlanning ? 'open' : 'closed'}</value>
                <value>ks-plan-TermsList-item</value>
                <value>@{#line.currentTermForView ? 'ks-plan-TermsList-item--current' : (!#line.currentTermForView and
                    #line.openForPlanning ? 'ks-plan-TermsList-item--future' : 'ks-plan-TermsList-item--past')}
                </value>
                <value>@{#line.openForPlanning ? 'ks-plan-TermsList-item--open' : 'ks-plan-TermsList-item--closed'}</value>
            </list>
        </property>
        <property name="layoutManager.lineGroupPrototype.style"
                  value="width:@{588/T(org.kuali.student.ap.framework.config.KsapFrameworkServiceLocator).getTermHelper().getNumberOfTermsInAcademicYear()}px;
                  margin:0 @{20/T(org.kuali.student.ap.framework.config.KsapFrameworkServiceLocator).getTermHelper().getNumberOfTermsInAcademicYear()}px;"/>
        <property name="layoutManager.lineGroupPrototype.header.upperGroup">
            <bean parent="Uif-HorizontalBoxGroup">

                <property name="items">
                    <list>
                        <bean p:id="@{#line.atpId.replaceAll('\.','-')}termnote_message" parent="Uif-InputField">
                            <property name="propertyName" value="lookupResults[@{#index}].termNoteUI" />
                            <property name="control">
                                <bean parent="Uif-TextAreaControl"
                                      p:watermarkText="No Term Notes" p:style="width: 97%; resize: none;">
                                    <property name="disabled" value="true"/>
                                </bean>
                            </property>

                        </bean>
                    </list>
                </property>
            </bean>
        </property>
        <property name="layoutManager.lineGroupPrototype.header.cssClasses">
            <list merge="true">
                <value>ks-plan-TermsList-itemHeader</value>
            </list>
        </property>
        <property name="header">
            <bean parent="Uif-HeaderFour" />
        </property>
        <property name="propertyName" value="lookupResults"/>
        <property name="cssClasses">
            <list merge="true">
                <value>myplan-carousel-list</value>
                <value>myplan-remove-margin</value>
                <value>ks-plan-TermsList</value>
            </list>
        </property>
        <property name="header.render" value="true"/>
        <property name="items">
            <list>
                <!-- This hidden field is only rendered for the ATP that should have focus in the UI. The value of "index" will
                     be used to initialize the javascript "carousellite" component. It should only be displayed for the PlanTerm
                     that should get focus -->
                <bean p:id="atp_start_index" parent="Uif-DataField" p:propertyName="index"
                      p:render="@{#line.index ge 0}" p:style="display:none;"/>

                <bean p:id="plan_base_atpId" parent="Uif-DataField" p:propertyName="atpIdYear" p:style="display: none;">
                    <property name="fieldLabel.render" value="false"/>
                </bean>

                <bean p:id="@{#line.atpId}_plan_status" parent="Uif-InputField" p:propertyName="openForPlanning">
                    <property name="fieldLabel.render" value="false"/>
                    <property name="control">
                        <bean parent="Uif-HiddenControl"/>
                    </property>
                </bean>
            </list>
        </property>
        <property name="subCollections">
            <list>
                <ref bean="plannedCollectionSnapshot"/>
                <ref bean="backupCollectionSnapshot"/>
            </list>
        </property>
        <property name="onDocumentReadyScript" value="
            truncateField('snapshot_courses_detail_list', true);
        "/>
    </bean>

    <bean id="plannedCollectionSnapshot" parent="Uif-StackedCollectionGroup" p:headerText="Planned">
        <property name="validationMessages.render" value="false"/>
        <property name="header">
            <bean parent="Uif-HeaderSix" p:additionalCssClasses="ks-plan-Bucket-header"/>
        </property>
        <property name="layoutManager">
            <bean parent="Uif-StackedCollectionLayout-WithBoxItems">
                <property name="cssClasses">
                    <list merge="true">
                        <value>ks-plan-Bucket-collection</value>
                    </list>
                </property>
                <property name="lineGroupPrototype">
                    <bean parent="Uif-CollectionVerticalBoxItem"/>
                </property>
            </bean>
        </property>
        <property name="dataAttributes">
            <map>
                <entry key="plantype" value="planned"/>
                <entry key="atpid" value="@{#line.atpId}"/>
                <entry key="size" value="@{#line.plannedList.size()}"/>
            </map>
        </property>
        <property name="cssClasses">
            <list merge="false">
                <value>myplan-carousel-term</value>
                <value>@{#line.atpId.replaceAll("\.","-")}</value>
                <value>myplan-term-planned</value>
                <value>@{#line.openForPlanning ? 'myplan-term-fixed-height' : ''}</value>
                <value>ks-plan-Bucket</value>
            </list>
        </property>
        <property name="layoutManager.lineGroupPrototype.footer.render" value="false"/>
        <property name="layoutManager.lineGroupPrototype.validationMessages.render" value="false"/>
        <property name="collectionObjectClass"
                  value="org.kuali.student.myplan.plan.dataobject.PlannedCourseDataObject"/>
        <property name="propertyName" value="plannedList"/>
        <property name="renderAddLine" value="false"/>
        <property name="renderLineActions" value="false"/>
        <property name="bindingInfo.bindToForm" value="true"/>
        <property name="disclosure.render" value="false"/>
        <property name="items">
            <list>
                <bean p:id="@{#line.planItemDataObject.id}_planned" parent="Uif-HorizontalFieldGroup"
                      p:render="@{#line.courseCode ne null}"
                      p:title="@{#line.courseTitle}">
                    <property name="group.validationMessages.render" value="false"/>
                    <property name="onClickScript"
                              value="var retrieveData = {action:'plan', viewId:'PlannedCourse-FormView', methodToCall:'startAddPlannedCourseForm', planItemId:'@{#line.planItemDataObject.id}', courseId:'@{#line.courseDetails.courseId}', pageId:'snapshot_course_summary_dialog_page'};
                                     var popupStyle = {width:'300px', height:'16px'};
                                     var popupOptions = {tail:{hidden:true}, align:'top', close:true};
                                     openPopup('snapshot_course_summary_dialog_page', retrieveData, 'plan', popupStyle, popupOptions, e);
                              "/>                    <property name="dataAttributes">
                        <map>
                            <entry key="courseid" value="@{#line.courseDetails.courseId}"/>
                            <entry key="planitemid" value="@{#line.planItemDataObject.id}"/>
                            <entry key="atpid" value="@{#line.planItemDataObject.atp}"/>
                        </map>
                    </property>
                    <property name="cssClasses">
                        <list merge="true">
                            <value>myplan-course-valid</value>
                            <value>@{#line.timeScheduleOpen ? 'schedule':''}</value>
                            <value>ks-plan-Bucket-item</value>
                            <value>ks-plan-Bucket-item--valid</value>
                            <value>ks-plan-Bucket-scheduleOpen</value>
                        </list>
                    </property>
                    <property name="items">
                        <list>
                            <bean parent="Uif-ImageField"
                                  p:source="@{#ConfigProperties['ks.myplan.externalizable.images.url']}pixel.gif"
                                  p:title="@{#line.showAlert ? #line.courseCode + ' is not scheduled for '+ #line.planItemDataObject.termName : #line.courseCode + ' is scheduled for '+ #line.planItemDataObject.termName}"
                                  p:altText="@{#line.showAlert ? #line.courseCode + ' is not scheduled for '+ #line.planItemDataObject.termName : #line.courseCode + ' is scheduled for '+ #line.planItemDataObject.termName}"
                                  p:render="@{#line.timeScheduleOpen}">
                                <property name="cssClasses">
                                    <list merge="true">
                                        <value>schedule-status</value>
                                        <value>@{#line.showAlert ? 'not-scheduled' : 'scheduled'}</value>
                                    </list>
                                </property>
                            </bean>
                            <bean parent="Uif-MessageField">
                                <property name="messageText" value="@{#line.courseCode}"/>
                                <property name="cssClasses">
                                    <list merge="true">
                                        <value>code</value>
                                        <value>ks-plan-Bucket-itemCode</value>
                                    </list>
                                </property>
                            </bean>
                            <bean parent="Uif-MessageField">
                                <property name="messageText" value="@{#line.courseDetails.credit}"/>
                                <property name="cssClasses">
                                    <list merge="true">
                                        <value>credit</value>
                                        <value>ks-plan-Bucket-itemCreditCount</value>
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
        <property name="collectionLookup.render" value="false"/>
        <property name="collectionLookup.quickfinderAction.render" value="false"/>
        <property name="footer">
            <bean parent="Uif-VerticalBoxGroup">
                <property name="validationMessages.render" value="false"/>
                <property name="items">
                    <list>
                        <bean parent="term_total_credits"
                              p:render="@{#line.academicRecord.size() gt 0 and #line.openForPlanning}">
                            <property name="cssClasses">
                                <list merge="true">
                                    <value>myplan-carousel-term-total</value>
                                    <value>ks-plan-Bucket-credit</value>
                                </list>
                            </property>
                            <property name="items">
                                <list>
                                    <ref bean="term_total_credits_label"/>
                                    <ref bean="term_total_credits_text"/>
                                </list>
                            </property>
                        </bean>
                        <bean parent="term_total_credits"
                              p:render="@{#line.academicRecord.size() eq 0 and #line.openForPlanning}">
                            <property name="cssClasses">
                                <list merge="true">
                                    <value>myplan-carousel-term-total</value>
                                    <value>ks-plan-Bucket-credit</value>
                                </list>
                            </property>
                            <property name="items">
                                <list>
                                    <ref bean="term_total_credits_label"/>
                                    <ref bean="term_total_credits_text"/>
                                </list>
                            </property>
                        </bean>
                        <bean parent="term_no_courses"
                              p:render="@{!#line.openForPlanning and #line.plannedList.size() eq 0}"/>
                    </list>
                </property>
            </bean>
        </property>
    </bean>

    <bean id="backupCollectionSnapshot" parent="Uif-StackedCollectionGroup" p:headerText="Backup">
        <property name="validationMessages.render" value="false"/>
        <property name="header">
            <bean parent="Uif-HeaderSix" p:additionalCssClasses="ks-plan-Bucket-header"/>
        </property>
        <property name="layoutManager">
            <bean parent="Uif-StackedCollectionLayout-WithBoxItems">
                <property name="cssClasses">
                    <list merge="true">
                        <value>ks-plan-Bucket-collection</value>
                    </list>
                </property>
                <property name="lineGroupPrototype">
                    <bean parent="Uif-CollectionVerticalBoxItem"/>
                </property>
            </bean>
        </property>
        <property name="dataAttributes">
            <map>
                <entry key="plantype" value="backup"/>
                <entry key="atpId" value="@{#line.atpId}"/>
                <entry key="size" value="@{#line.backupList.size()}"/>
            </map>
        </property>
        <property name="cssClasses">
            <list merge="false">
                <value>myplan-carousel-term</value>
                <value>@{#line.atpId.replaceAll("\.","-")}</value>
                <value>myplan-term-backup</value>
                <value>ks-plan-Bucket</value>
            </list>
        </property>
        <property name="layoutManager.lineGroupPrototype.footer.render" value="false"/>
        <property name="layoutManager.lineGroupPrototype.validationMessages.render" value="false"/>
        <property name="collectionObjectClass"
                  value="org.kuali.student.myplan.plan.dataobject.PlannedCourseDataObject"/>
        <property name="propertyName" value="backupList"/>
        <property name="renderAddLine" value="false"/>
        <property name="renderLineActions" value="false"/>
        <property name="bindingInfo.bindToForm" value="true"/>
        <property name="disclosure.render" value="false"/>
        <property name="items">
            <list>
                <bean p:id="@{#line.planItemDataObject.id}_backup" parent="Uif-HorizontalFieldGroup"
                      p:title="@{#line.courseTitle}"
                      p:render="@{#line.courseCode ne null}">
                    <property name="group.validationMessages.render" value="false"/>
                    <property name="onClickScript"
                              value="var retrieveData = {action:'plan', viewId:'PlannedCourse-FormView', methodToCall:'startAddPlannedCourseForm', planItemId:'@{#line.planItemDataObject.id}', courseId:'@{#line.courseDetails.courseId}', pageId:'snapshot_course_summary_dialog_page'};
                                     var popupStyle = {width:'300px', height:'16px'};
                                     var popupOptions = {tail:{hidden:true}, align:'top', close:true};
                                     openPopup('snapshot_course_summary_dialog_page', retrieveData, 'plan', popupStyle, popupOptions, e);
                              "/>                    <property name="dataAttributes">
                        <map>
                            <entry key="courseid" value="@{#line.courseDetails.courseId}"/>
                            <entry key="planitemid" value="@{#line.planItemDataObject.id}"/>
                            <entry key="atpid" value="@{#line.planItemDataObject.atp}"/>
                        </map>
                    </property>
                    <property name="cssClasses">
                        <list merge="true">
                            <value>myplan-course-valid</value>
                            <value>@{#line.timeScheduleOpen ? 'schedule':''}</value>
                            <value>ks-plan-Bucket-item</value>
                            <value>ks-plan-Bucket-item--valid</value>
                            <value>@{#line.timeScheduleOpen ? 'ks-plan-Bucket-item--scheduleOpen':''}</value>
                        </list>
                    </property>
                    <property name="items">
                        <list>
                            <bean parent="Uif-ImageField"
                                  p:source="@{#ConfigProperties['ks.myplan.externalizable.images.url']}pixel.gif"
                                  p:title="@{#line.showAlert ? #line.courseCode + ' is not scheduled for '+ #line.planItemDataObject.termName : #line.courseCode + ' is scheduled for '+ #line.planItemDataObject.termName}"
                                  p:altText="@{#line.showAlert ? #line.courseCode + ' is not scheduled for '+ #line.planItemDataObject.termName : #line.courseCode + ' is scheduled for '+ #line.planItemDataObject.termName}"
                                  p:render="@{#line.timeScheduleOpen}">
                                <property name="cssClasses">
                                    <list merge="true">
                                        <value>schedule-status</value>
                                        <value>@{#line.showAlert ? 'not-scheduled' : 'scheduled'}</value>
                                    </list>
                                </property>
                            </bean>
                            <bean parent="Uif-MessageField">
                                <property name="messageText" value="@{#line.courseCode}"/>
                                <property name="cssClasses">
                                    <list merge="true">
                                        <value>code</value>
                                        <value>ks-plan-Bucket-itemCode</value>
                                    </list>
                                </property>
                            </bean>
                            <bean parent="Uif-MessageField">
                                <property name="messageText" value="@{#line.courseDetails.credit}"/>
                                <property name="cssClasses">
                                    <list merge="true">
                                        <value>credit</value>
                                        <value>ks-plan-Bucket-itemCreditCount</value>
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
        <property name="collectionLookup.render" value="false"/>
        <property name="collectionLookup.quickfinderAction.render" value="false"/>
        <property name="footer">
            <bean parent="term_no_courses"
                  p:render="@{!#line.openForPlanning AND #line.backupList.size() eq 0}"/>
        </property>
    </bean>

    <bean id="SnapshotBookmarkSummary-LookupView" parent="MyPlanBaseLookupView">
        <property name="viewName" value="SnapshotBookmarkSummary-LookupView"/>
        <property name="headerText" value="Bookmarked Courses"/>
        <property name="page.header.render" value="false"/>
        <property name="header.render" value="false"/>
        <property name="footer.render" value="false"/>
        <property name="dataObjectClassName" value="org.kuali.student.myplan.plan.dataobject.PlannedCourseDataObject"/>
        <property name="viewHelperServiceClass"
                  value="org.kuali.student.myplan.plan.service.SavedCoursesLookupableHelperImpl"/>
        <property name="singlePageView" value="true"/>
        <property name="cssClasses">
            <list merge="true">
                <value>myplan-view</value>
            </list>
        </property>
        <property name="criteriaFields">
            <list/>
        </property>
        <property name="resultFields">
            <list/>
        </property>

        <!-- Not Displaying any criteria -->
        <property name="criteriaGroup">
            <bean parent="Uif-VerticalBoxGroup">
                <property name="validationMessages.render" value="false"/>
            </bean>
        </property>
        <property name="resultsGroup">
            <bean id="snapshot_bookmarks_summary" parent="snapshot_bookmarks_summary_list"/>
        </property>
    </bean>

    <bean id="snapshot_bookmarks_summary_list" parent="Uif-ListCollectionSection" p:headerText="Bookmarked Courses">
        <property name="header">
            <bean parent="Uif-HeaderSix"/>
        </property>
        <property name="cssClasses">
            <list merge="true">
                <value>myplan-saved-courses-summary</value>
            </list>
        </property>
        <property name="collectionObjectClass"
                  value="org.kuali.student.myplan.plan.dataobject.PlannedCourseDataObject"/>
        <property name="propertyName" value="lookupResults"/>
        <property name="renderAddLine" value="false"/>
        <property name="renderLineActions" value="false"/>
        <property name="disclosure.render" value="false"/>
        <property name="layoutManager.lineGroupPrototype.header.render" value="false"/>
        <property name="onDocumentReadyScript" value="truncateField('snapshot_bookmarks_summary');"/>
        <property name="items">
            <list>
                <bean parent="Uif-HorizontalFieldGroup" p:id="@{#line.planItemDataObject.id}" p:style="cursor:pointer;">
                    <property name="fieldLabel.render" value="false"/>
                    <property name="group.validationMessages.render" value="false"/>
                    <property name="cssClasses">
                        <list merge="true">
                            <value>myplan-click</value>
                            <value>uif-tooltip</value>
                        </list>
                    </property>
                    <property name="onClickScript"
                              value="var retrieveData = {action:'plan', viewId:'PlannedCourse-FormView', methodToCall:'startAddPlannedCourseForm', planItemId:'@{#line.planItemDataObject.id}', courseId:'@{#line.courseDetails.courseId}', pageId:'snapshot_course_summary_dialog_page'};
                                     var popupStyle = {width:'300px', height:'16px'};
                                     var popupOptions = {tail:{hidden:true}, align:'top', close:true};
                                     openPopup('snapshot_course_summary_dialog_page', retrieveData, 'plan', popupStyle, popupOptions, e);
                              "/>
                    <property name="items">
                        <list>
                            <bean parent="Uif-Message" p:messageText="@{#line.courseDetails.code}">
                                <property name="style" value="margin-right:5px;"/>
                            </bean>
                            <bean p:id="@{#line.planItemDataObject.id}_saved" parent="Uif-Message"
                                  p:messageText="@{#line.courseDetails.courseTitle}">
                                <property name="style" value="margin-right:5px;"/>
                                <property name="cssClasses">
                                    <list merge="true">
                                        <value>myplan-text-ellipsis</value>
                                        <value>myplan-text-gray</value>
                                    </list>
                                </property>
                            </bean>
                            <bean parent="Uif-Message" p:messageText="(@{#line.courseDetails.credit})">
                                <property name="style" value="margin-right:0px;"/>
                                <property name="cssClasses">
                                    <list merge="true">
                                        <value>myplan-text-gray</value>
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="snapshot_course_summary_dialog_page" parent="KSAP-PopoverPage" p:id="snapshot_course_summary_dialog_page" p:headerText="View Course Summary">
        <property name="items">
            <list>
                <bean parent="course_title_link"/>
                <bean parent="Uif-VerticalBoxGroup" p:style="margin:0px;">
                    <property name="validationMessages.render" value="false"/>
                    <property name="items">
                        <list>
                            <ref bean="course_details_summary_data"/>
                            <bean parent="KSAP-SimpleInputField" p:propertyName="courseNote"
                                  p:defaultValue="@{#form.planItemId NE null ? #form.courseNote : ''}">
                                <property name="label" value="Note for course"/>
                                <property name="fieldLabel.render" value="true"/>
                                <property name="fieldLabel.cssClasses">
                                    <list merge="true">
                                        <value>myplan-text-normal</value>
                                    </list>
                                </property>
                                <property name="labelPlacement" value="TOP"/>

                                <property name="control">
                                    <bean parent="Uif-TextAreaControl"
                                          p:watermarkText="Enter a short note (500 characters max)" p:maxLength="500"
                                          p:style="resize:none; width:97%;">
                                        <property name="disabled" value="true"/>
                                    </bean>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

</beans>
